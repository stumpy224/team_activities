<h1>Listing nominations</h1>

<table>
  <thead>
    <tr>
      <th>Restaurant</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nominations.each do |nomination| %>
      <tr>
        <td style="padding-right:2em;"><%= nomination.restaurant_id %></td>
        <td style="padding-right:2em;"><%= nomination.year_id %></td>
        <td><%= link_to 'Edit', edit_nomination_path(nomination) %>&nbsp;|&nbsp;</td>
        <td><%= link_to 'Destroy', nomination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nomination', new_nomination_path %>

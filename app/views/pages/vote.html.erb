      <div class="form-group">
        <label for="votingMember">Who's voting?</label>
        <input type="email" class="form-control" id="identifier" placeholder="Enter ACID">
      </div>
      <div class="form-group">
        <div class="dropdown">
          <button class="btn btn-default dropdown-toggle" type="button" id="vote1Dropdown" data-toggle="dropdown">
            1st Vote
            <span class="caret" />

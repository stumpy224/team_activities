<!DOCTYPE html>
<html>
  <head>
    <title>E-Business Holiday Meal</title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag 'http://www.infarmbureau.com/favicon.ico', rel: 'icon' %>
    <%= stylesheet_link_tag 'http://www.infarmbureau.com/favicon.ico', rel: 'shortcut icon' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <% if flash[:danger] %>
        <div class="alert-danger"><%= flash[:danger] %></div>
      <% end %>
      
      <%= yield %>
    </div>
  </body>
</html>
